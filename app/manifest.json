{
  "name": "wokitoki",
  "description": "like wokitoki",
  "version": "0.1.0",
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "images/*.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "*"
      ],
      "css": [
        "styles/styles.css"
      ],
      "js": [
        "src/content-script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "wokitoki",
    "default_icon": "images/logo-48.png"
  },
  "icons": {
    "48": "images/logo-48.png",
    "96": "images/logo-96.png",
    "128": "images/logo-128.png"
  },
  "manifest_version": 2
}
