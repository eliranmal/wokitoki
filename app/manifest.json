{
  "name": "wokitoki",
  "description": "it's like wokitoki",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "wokitoki",
    "default_icon": "images/logo-48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "images/logo-48.png",
    "96": "images/logo-96.png",
    "128": "images/logo-128.png"
  }
}
